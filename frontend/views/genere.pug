extends layout

block content
  div.w3-container
    h1= titulo

    div.w3-panel.w3-border
      p ID: #{genere._id}
      p Description Level: #{genere.DescriptionLevel}
      p Entity Type: #{genere.EntityType}
      p Complete Unit ID: #{genere.CompleteUnitId}
      p Repository Code: #{genere.RepositoryCode}
      p Country Code: #{genere.CountryCode}
      p Title Type: #{genere.UnitTitleType}
      p Title: #{genere.UnitTitle}
      p Alternative Title: #{genere.AlternativeTitle}
      p Initial Date: #{genere.UnitDateInitial}
      p Final Date: #{genere.UnitDateFinal}
      p Author: #{genere.Author}
      p Contributor: #{genere.Contributor}
      p Dimensions: #{genere.Dimensions}
      p Repository: #{genere.Repository}
      p Producer: #{genere.Producer}
      p Scope Content: #{genere.ScopeContent}
      p Access Restrictions: #{genere.AccessRestrict}
      p Use Restrictions: #{genere.UseRestrict}
      p Physical Location: #{genere.PhysLoc}
      p Status: #{genere.Status}
      p Retention Disposal: #{genere.RetentionDisposalDocumentState}
      // Include other fields as necessary

    a.w3-button.w3-blue(href="/") Return to Main Page

  footer.w3-container.w3-green 
    address Generated by Grupo12::EngWeb2024 on #{data}
