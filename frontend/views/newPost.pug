extends layout

block content
  .w3-card-4 
    header.w3-container.w3-light-grey
      h1= titulo 
    .w3-container
        form.w3-container(method="POST")
            fieldset
                legend Metadata
                label ID
                input.w3-input.w3-round(type="text" name="_id") 
                label UserID 
                input.w3-input.w3-round(type="text" name="UserId")
                label InqId 
                input.w3-input.w3-round(type="text" name="InqId")
                label PostTile 
                input.w3-input.w3-round(type="text" name="PostTitle")
                label PostText 
                input.w3-input.w3-round(type="text" name="PostText")


            button.w3-btn.w3-green.w3-mb-2(type="submit") Register 
    footer.w3-container.w3-grey
        address Generated by Grupo12::EngWeb2024 on #{data}




